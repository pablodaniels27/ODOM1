<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="Lector.MainFormController" alignment="CENTER" spacing="10.0">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>

    <Button fx:id="enrollButton" text="Enroll Fingerprint" onAction="#handleEnrollButtonAction"/>
    <Button fx:id="verifyButton" text="Verify Fingerprint" onAction="#handleVerifyButtonAction"/>
</VBox>

